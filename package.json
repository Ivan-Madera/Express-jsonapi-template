{
  "name": "express-jsonapi-template",
  "version": "1.0.0",
  "description": "Template de NodeJS para desarrollos cortos y sencillos implementando las respuestas con jsonapi",
  "main": "./app.ts",
  "scripts": {
    "build": "tsc",
    "start": "node build/app.js",
    "dev": "ts-node-dev app.ts",
    "jest": "npx jest",
    "new:migration": "npx sequelize-cli migration:generate --name migration-skeleton",
    "new:seeder": "npx sequelize-cli seed:generate --name seeder-skeleton",
    "migrate": "npx sequelize-cli db:migrate",
    "seeder": "npx sequelize-cli db:seed:all",
    "migrate:undo": "npx sequelize-cli db:migrate:undo:all",
    "seeder:undo": "npx sequelize-cli db:seed:undo:all"
  },
  "keywords": ["express", "jsonapi", "typescript", "sequelize", "mysql", "swagger"],
  "author": "Ivan Madera",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.24",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "log4js": "^6.9.1",
    "mysql2": "^3.9.2",
    "sequelize": "^6.37.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
